(Vuex)\n\nexport default new Vuex.Store({\n  state: {\n    path: []\n  },\n  mutations: {\n    setPath(state, play) {\n      console.log(play, 'pay')\n      let l = play.length;\n      let child = play[l - 1]\n      if (l > 1) {\n        play.length--\n        if (child) {\n          let routesLength = routes.length;\n          while (routesLength--) {\n            if (routes[routesLength].path === child) {\n              child = routes[routesLength].name\n            }\n          }\n        }\n         // 变更状态\n        state.path = [...play, child]\n      } else {\n        state.path = [...play]\n      }\n      // console.log(state.path, '== state.path ==')\n    }\n  },\n  actions: {\n  },\n  modules: {\n  }\n})\n"]}]}