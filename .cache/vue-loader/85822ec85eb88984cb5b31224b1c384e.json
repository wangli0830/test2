t default {\n  name: \"HelloWorld\",\n  props: {  //接收参数\n    msg: String,\n  },\n\n  data() {\n    return {\n      input: \"\",\n      options: [\n        {\n          value: \"选项1\",\n          label: \"黄金糕\",\n        },\n        {\n          value: \"选项2\",\n          label: \"双皮奶\",\n        },\n        {\n          value: \"选项3\",\n          label: \"蚵仔煎\",\n        },\n        {\n          value: \"选项4\",\n          label: \"龙须面\",\n        },\n        {\n          value: \"选项5\",\n          label: \"北京烤鸭\",\n        },\n      ],\n      value: \"\",\n    };\n  },\n  methods: {\n    addressSub() {\n      test({\n      name:this.value\n      }).then((res) => {\n        console.log(res, \"res\");\n        alert(res.msg||\"没找到msg\")\n        this.$message({\n                  message: '恭喜你，这是一条成功消息',\n                  type: 'success'\n                });\n\n\n\n      });\n    },\n    nameSub() {\n      test1().then((res) => {\n        console.log(res, \"res\");\n      });\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n\n\n\n<style scoped>\n.hello {\n  padding: 20px;\n  text-align: left;\n}\n.ul {\n  margin-top: 10px;\n  margin-bottom: 10px;\n}\n.ul .el-input {\n  width: 300px;\n  margin-right: 20px;\n}\n.hello .el-select {\n  margin-right: 20px;\n}\n</style>\n"]}]}